
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <script src="../Assets/assets/js/jquery-3.6.1.js"></script>
    @*<script src="https://code.jquery.com/jquery-3.6.1.js"></script>*@
    <script type="text/javascript" src="../Assets/assets/js/jquery.dataTables.min.js" defer></script>
    @*<script type="text/javascript" src=" https://cdn.datatables.net/1.10.13/js/jquery.dataTables.min.js" defer></script>*@
    <script src="../Assets/assets/js/script.js"></script>
    <link rel="stylesheet" href="../Assets/assets/css/jquery.dataTables.min.css">
    @*<link rel="stylesheet" href="https://cdn.datatables.net/1.10.13/css/jquery.dataTables.min.css">*@
    <link href="../Assets/assets/css/materialdesignicons.min.css" rel="stylesheet" />
    @*<link href="https://cdn.materialdesignicons.com/4.4.95/css/materialdesignicons.min.css" rel="stylesheet" />*@
    <link rel="stylesheet" href="../Assets/assets/css/font-awesome.css" />
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" />*@

</head>
<input type="hidden" id="url_local" value="@System.Configuration.ConfigurationManager.AppSettings["url_local"]" />
<body>
    <div class="main main-app p-3 p-lg-4">

        <div class="row">
            <div class="loader" style="display: none;"></div>
            <div class="col-12">
                <div class="card card-default">
                    <div class="card-header card-header-border-bottom d-flex justify-content-between">
                        <h2>Anonymous Items</h2>
                    </div>
                    <div class="card-body">
                        <div style="color:black" class="table-responsive hoverable-data-table">
                            <table id="tblAnonymous" class="table table-hover nowrap table-striped table-bordered" style="width:100%">
                                <thead>
                                    <tr>
                                        <th style='display:none'>GUID</th>
                                        <th>Item Barcode</th>
                                        <th>Item Description</th>
                                        <th>Inventory Name</th>
                                        <th>Device Name</th>
                                        <th style="display: none;">ItemQty</th>
                                        <th style="display: none;">Image</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>

                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        @*modelpopup*@
        <div class="modal fade" id="modal-lg" tabindex="-1" role="dialog" aria-labelledby="exampleModalLarge" aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header header">
                        <button type="button" id="cls" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <h2 class="modal-title" id="exampleModalLarge2" style="color:white">Scale</h2>

                    </div>
                    <div class="modal-body">

                        <div class="row">
                            <div class="col-sm-6 " style="display: none;">
                                <label>GUID</label>
                                <input class="form-control" placeholder="" type="text" maxlength="100" id="guid" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <label>Barcode</label>
                                <input class="form-control" type="text" id="barcode" />
                            </div>
                            <div class="col-sm-6">
                                <label>En Name</label>
                                <input class="form-control" type="text" id="name" />
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <label>Item Qty</label>
                                <input class="form-control" type="number" id="itemQty" />
                            </div>
                            <div class="col-sm-6">

                            </div>

                        </div>
                        <div class="row">
                            <div class="col-sm-4">

                            </div>
                            <div class="col-sm-4">
                                <div class="loader" style="display: none;"></div>
                                <input id="image" style="display: none;" />
                                <img id="img" style="width:60%; margin-top: 20px;" />
                            </div>
                            <div class="col-sm-4">

                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" id="btnclose" class="btn btn-danger btn-pill" data-dismiss="modal">Close</button>
                        <button type="button" id="btnsave" title="Save" class="btn btn-primary btn-pill">Save</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="../Assets/assets/contents/js/AnonymousItems.js"></script>
    <script src="../Assets/assets/contents/js/MenuOptions.js"></script>
    <script src="../Assets/assets/contents/js/GenericAjax.js"></script>
    <script src="../Assets/assets/contents/js/sweetalert.min.js"></script>
    <script src="../Assets/assets/js/bootstrap-datepicker.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>*@
    <link rel="stylesheet" href="../Assets/assets/css/bootstrap-datepicker.min.css">
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css">*@
    <script src="../Assets/assets/js/bootstrap.min.js"></script>
    @*<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>*@

</body>
